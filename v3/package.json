{
  "name": "neact",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/neact.js neact.js.map neact.min.js lib/*",
    "minify": "uglifyjs dist/neact.js -c collapse_vars,evaluate,screw_ie8,unsafe,loops=false,keep_fargs=false,pure_getters,unused,dead_code -m -o dist/neact.min.js -p relative --in-source-map dist/neact.js.map --source-map dist/neact.min.js.map",
    "build:dist": "rollup -c",
    "build:cjs": "babel src -d lib",
    "build:es": "babel --no-babelrc src -d es",
    "build": "npm-run-all clean build:dist build:cjs build:es minify",
    "release": "cross-env karma start --single-run && git commit -am %npm_package_version% && git tag %npm_package_version% && git push && git push --tags",
    "test": "karma start"
  },
  "author": "Nobo Zhou <nobo.zhou@foxmail.com>",
  "bugs": "https://github.com/bplok20010/neact/issues",
  "homepage": "https://github.com/bplok20010/neact",
  "repository": "https://github.com/bplok20010/neact",
  "files": [
    "*.md",
    "lib",
    "dist",
    "es",
    "index.js"
  ],
  "keywords": [
    "neact",
    "react",
    "react-dom",
    "virtual dom",
    "components",
    "vdom",
    "virtual",
    "jsx",
    "js"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "chai-sinon": "^2.8.1",
    "cross-env": "^3.1.4",
    "jasmine-core": "^2.5.1",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-json": "^2.1.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "uglify-js": "^2.7.5",
    "webpack": "^1.13.2"
  }
}
